		<class name="ReceiptCustomItem" title="收款单自定义项" tableName="ar_receipt_customitem">
			<attributes>
				<attribute name="id" columnName="id" title="收款单id" type="Receipt" isKey="true" />
		   </attributes>
		</class>
		
		<class name="ReceiptDetailCustomItem" title="收款单明细自定义项" tableName="ar_receiptDetail_customitem">
