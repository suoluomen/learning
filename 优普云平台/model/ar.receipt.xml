				<attribute name="id" columnName="id" title="收款单明细id" type="ReceiptDetail" isKey="true" />
		   </attributes>
		</class>	
		<realizations>
			<realization supplier="base.itf.CustomItem" client="ReceiptCustomItem" />
			<realization supplier="base.itf.CustomItem" client="ReceiptDetailCustomItem" />
			
		    <realization supplier="voucher.base.IVouchOrg" client="Receipt" />
			<!--note="引入应收单客户信息"-->
			<realization supplier="ar.itf.IARCustomer" client="Receipt" />
			<!--note="引入应收单部门、业务员"-->
			<realization supplier="ar.itf.IARInventoryLine" client="ReceiptDetail"  />
			<!--note="引入应收单项目"-->
			<realization supplier="ar.itf.IARItemLine" client="ReceiptDetail" />
		</realizations>
		
		<generalizations>
			<generalization parent="ar.abs.ARVouch" child="Receipt" />
			<generalization parent="ar.abs.ARVouchLine" child="ReceiptDetail" />
		</generalizations>
		
		<associations>
			<association type="composition" roleB="belongReceipt" typeB="Receipt" roleA="receiptDetails" typeA="ReceiptDetail"/>	
			<association type="composition" roleB="id" typeB="Receipt" roleA="headItem" typeA="ReceiptCustomItem" roleAMulti="1" />
			<association type="composition" roleB="id" typeB="ReceiptDetail" roleA="bodyItem" typeA="ReceiptDetailCustomItem" roleAMulti="1" />
